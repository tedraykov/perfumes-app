{
	"name": "perfume-app",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"codegen:appwrite": "fetch-appwrite-types includeDBName outDir ./src/lib/server/appwrite outFileName codegen hardTypes",
		"codegen:supabase": "supabase gen types typescript --project-id aijkqebokminzwqntwnv --schema public > src/lib/server/supabase/types.ts"
	},
	"devDependencies": {
		"@lucide/svelte": "^0.483.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/cheerio": "^0.22.35",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^1.3.13",
		"clsx": "^2.1.1",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.15.0",
		"lucide-svelte": "^0.460.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.25.0",
		"svelte-check": "^4.1.5",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.2",
		"vite": "^5.4.14",
		"vitest": "^2.1.9"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.49.1",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"appwrite": "14.0.1",
		"cheerio": "^1.0.0",
		"fetch-appwrite-types": "^2.2.2",
		"node-appwrite": "^14.2.0"
	}
}
